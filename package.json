{
  "name": "zeroconf-redux",
  "version": "0.9.0",
  "description": "is a minimal redux dev stack, on top of browserify + budo",
  "engines": {
    "npm": ">= 3"
  },
  "scripts": {
    "cp_babelrc": "[ -f ../../package.json ] && [ ! -f ../../.babelrc ] && cp .babelrc ../.. || exit 0",
    "example": "budo ./example/index.js:bundle.js --dir ./example --live --open -- -t babelify",
    "postinstall": "npm run cp_babelrc",
    "postversion": "git push origin v${npm_package_version}; npm publish; git push origin master",
    "test": "echo \"no test :P\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/zeroconf-redux.git"
  },
  "keywords": [
    "browserify",
    "budo",
    "react",
    "redux"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/zeroconf-redux/issues"
  },
  "homepage": "http://npm.im/zeroconf-redux",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "budo": "^9.2.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0"
  },
  "devDependencies": {}
}
