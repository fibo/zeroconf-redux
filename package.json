{
  "name": "zeroconf-redux",
  "version": "0.2.1",
  "description": "is a minimal redux dev stack, on top of browserify + budo",
  "engines": {
    "npm": ">= 4"
  },
  "scripts": {
    "prealert": "npm outdated",
    "alert": "echo \"\nDependencies need update, run\n\n\tnpm run update-deps\n\"",
    "cp_babelrc": "[[ -f ../../package.json ]] && [[ ! -f ../../.babelrc ]] && cp .babelrc ../.. || exit 0",
    "postversion": "git push origin v${npm_package_version}; npm publish; git push origin master",
    "postinstall": "npm run cp_babelrc",
    "pretest": "npm install",
    "test": "[[ $(npm outdated) ]] && npm run alert || exit 0",
    "update-deps": "npm update --save --dev",
    "postupdate-deps": "git commit -am 'updated deps'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/zeroconf-redux.git"
  },
  "keywords": [
    "browserify",
    "budo",
    "redux"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/zeroconf-redux/issues"
  },
  "homepage": "http://npm.im/zeroconf-redux",
  "dependencies": {
    "babel-preset-es2015": "^6.2.0",
    "babel-preset-react": "^6.2.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "budo": "^8.3.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "livereactload": "^2.2.3",
    "react-proxy": "^1.1.8"
  }
}
