{
  "name": "zeroconf-redux",
  "version": "4.5.0",
  "description": "is a minimal React/Redux dev stack, on top of browserify + budo",
  "bin": "./cli.js",
  "engines": {
    "npm": ">= 5"
  },
  "scripts": {
    "copy_babelrc": "node copy.js .babelrc",
    "copy_browserslistrc": "node copy.js .browserslistrc",
    "copy_editorconfig": "node copy.js .editorconfig",
    "example_counter": "budo ./examples/counter/index.js:bundle.js --dir ./examples/counter --open -- -t babelify",
    "postinstall": "npm run copy_babelrc; npm run copy_browserslistrc; npm run copy_editorconfig",
    "postversion": "git push origin v${npm_package_version}; npm publish; git push origin master",
    "test": "node notest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/zeroconf-redux.git"
  },
  "keywords": [
    "browserify",
    "budo",
    "react",
    "redux",
    "fast",
    "development",
    "prototyping",
    "zero",
    "conf",
    "config",
    "configuration"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/zeroconf-redux/issues"
  },
  "homepage": "http://g14n.info/zeroconf-redux/",
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0",
    "budo": "^11.6.1",
    "cross-env": "^5.2.0",
    "envify": "^4.1.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "uglify-js": "^3.4.9",
    "uglifyify": "^5.0.1"
  },
  "devDependencies": {
    "dot-editorconfig": "^1.1.1"
  }
}
